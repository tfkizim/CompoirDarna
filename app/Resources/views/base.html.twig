                                            <button data-val="2" class="btn btn-primary btn-ym btn-pax active" type="button">2</button>
                                            <button data-val="3" class="btn btn-primary btn-ym btn-pax" type="button">3</button>
                                            <button data-val="4" class="btn btn-primary btn-ym btn-pax" type="button">4</button>
                                            <input type="number" data-parsley-type="integer" name="book[pax]" data-parsley-trigger="change blur keyup" id="book_pax" value="2" min="1" required>
                                        </div>
                                    </div>
