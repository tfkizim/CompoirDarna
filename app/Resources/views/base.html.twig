            <div id="book-update-hour" class="uk-modal">
                <div class="uk-modal-dialog">
                    <button type="button" class="uk-modal-close uk-close"></button>
                    <form action="" method="post">
                        <div class="uk-grid" data-uk-grid-margin="">
                            <div class="uk-width-medium-1-1">
                                <div class="parsley-row">
                                    <label for="bookupdate_hour" class="uk-required">Heure<span class="req">*</span></label><br>
                                    <input type="hidden" name="bookupdate[id]" id="bookupdate_id">
                                    <select id="bookupdate_hour" name="bookupdate[hour]" class="select-ym" required>
                                        {% for service in services %}
                                            {% set servicevaleur=service.valeur|json_decode() %}
                                            {% set openhour=servicevaleur.Lundi.OPENHOUR %}
                                            {% set closehour=servicevaleur.Lundi.CLOSEHOUR %}
                                            {% set interval=servicevaleur.Lundi.INTERVAL %}
                                            {% set openhourtimestamp=(("1970-01-01 "~openhour)|date("U")) %}
                                            {% set closehourtimestamp=(("1970-01-01 "~closehour)|date("U")) %}
                                            {% for hour in range(low=openhourtimestamp, high=closehourtimestamp, step=interval) %}
                                                <option value="{{ service.id }}|{{ hour|date("H\:i") }}">{{ hour|date("H\:i") }}</option>
                                            {% endfor %}
                                        {% endfor %}
                                    </select>
                                </div>
                            </div>
                            <div class="uk-width-medium-1-1 uk-text-center">
                                <br><br>
                                <button type="button" class="uk-button uk-button-success uk-button-large" id="bookupdatehour" data-id="">Modifier</button>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
            <div id="modal-plan-container" class="uk-modal">
                <div class="uk-modal-dialog uk-modal-dialog-large">
                    <button type="button" class="uk-modal-close uk-close"></button>
                    <div class="uk-grid" data-uk-grid-margin="">
                        <div class="uk-width-medium-1-1 uk-text-center">
                            <button class="uk-button uk-button-success uk-button-large" id="select-tables" data-id="">Seléctionner</button>
                        </div>
                        <div class="uk-width-medium-1-1">
                            <ul class="uk-tab" data-uk-tab="{connect:'#plans-tables'}" id="plans_table_1">
                                {% for floor in floors %}
                                <li><a href="#" class="trigfloortab" data-floor="{{ floor.id }}">{{ floor.name }}</a></li>
                                {% endfor %}
                            </ul>
                            <ul id="plans-tables" class="uk-switcher uk-margin">
                                {% for floor in floors %}
                                    <li>
                                        <div class="modal-plan">
                                            {# set sfloor="" %}
                                            {% for floor in floors %}
                                                {% if selectedfloor is defined and floor.id == selectedfloor.id %}
                                                    {% set sfloor=floor %}
                                                {% endif %}
                                            {% endfor %}
                                            {% if sfloor is empty %}
                                                {% set sfloor=floors|first %}
                                            {% endif #}

                                            {# for grouptabl in sfloor.grouptabls %}
                                            <label for="group_{{ grouptabl.id }}">
                                                <div class="modal-grouptable table group-table uk-button-dropdown group-{{grouptabl.orientation}}" style="left: {{grouptabl.leftp}}%;top: {{grouptabl.topp}}%;" data-id="{{grouptabl.id}}" data-uk-dropdown="{mode:'click'}">
                                                    {% set namegroup="" %}
                                                    {% set nbrcovert=0 %}
                                                    {% for tabl in grouptabl.tabls %}
                                                        {% set namegroup=namegroup~"+"~tabl.name %}
                                                        {% set nbrcovert=nbrcovert+tabl.nbrChaire %}
                                                    {% endfor %}
                                                    <span class="title">{{ namegroup[1:] }}</span>
                                                    <span class="uk-badge uk-badge-notification nbr-covert">{{nbrcovert}}</span>
                                                    {% for tabl in grouptabl.tabls %}
                                                        {% set classe="" %}
                                                        {% set classeid="" %}
                                                        {% for typetabl in tabl.tabltypetabls %}
                                                            {% set classe=classe~" "~typetabl.typeTablId.class %}
                                                            {% set classeid=classeid~","~typetabl.typetablid.id %}
                                                        {% endfor %}
                                                        {% if classe is empty %}
                                                            {% set classe=" square" %}
                                                        {% endif %}
                                                        <div class="sub-table{{ classe }}" data-id="{{ tabl.id }}" data-name="{{ tabl.name }}"></div>
                                                    {% endfor %}
                                                    <input type="checkbox" name="checkbox_demo" class="table-checkbox-id" id="group_{{ grouptabl.id }}" data-md-icheck />
                                                </div>
                                            </label>
                                            {% endfor #}
                                            {% for tabl in floor.tabls %}
                                                {% if tabl.grouptablId is null %}
                                                    {% set classe="" %}
                                                    {% set classeid="" %}
                                                    {% for typetabl in tabl.tabltypetabls %}
                                                        {% set classe=classe~" "~typetabl.typetablid.class %}
                                                        {% set classeid=classeid~","~typetabl.typetablid.id %}
                                                    {% endfor %}
                                                    {% set status="" %}

                                                    {% if booktables[tabl.id] is defined %}
                                                        {% set i=0 %}
                                                        {% for book in booktables[tabl.id] %}
                                                            {% if i == 0 %}
                                                                {% set timestamp='data-timestamp="'~book.dateBook|date("U")~'"' %}
                                                                {% if book.stateId %}
                                                                    {% set status='md-bg-'~book.stateId.color %}
                                                                {% endif %}
                                                            {% endif %}
                                                            {% set i=i+1 %}
                                                        {% endfor %}
                                                    {% endif %}
                                                    <label for="tabl_{{ tabl.id }}">
                                                        <div class="modal-table table{{classe}}" data-id="{{ tabl.id }}" data-name="{{ tabl.name }}" style="left: {{ tabl.leftp }}%;top: {{ tabl.topp }}%;">
                                                            <span class="title">{{ tabl.name }}</span>
                                                            <span class="uk-badge uk-badge-notification nbr-covert">{{ tabl.nbrChaire }}</span>
                                                            <input type="checkbox" name="checkbox_demo" class="table-checkbox-id" id="tabl_{{ tabl.id }}" data-md-icheck />
                                                            <div class="hidden info_table"
                                                                 data-name="{{tabl.name}}"
                                                                 data-nbrChaire="{{tabl.nbrChaire}}"
                                                                 data-typeTablId="{{classeid}}"
                                                                 data-leftp="{{tabl.leftp}}"
                                                                 data-topp="{{tabl.topp}}"
                                                                 data-rotation="{{tabl.rotation}}"
                                                            ></div>
                                                        </div>
                                                    </label>
                                                {% endif %}
                                            {% endfor %}
                                            <!-- Grids -->
                                            <div class="grid grid1"></div>
                                            <div class="grid grid2"></div>
                                            <div class="grid grid3"></div>
                                            <div class="grid grid4"></div>
                                        </div>
                                    </li>
                                {% endfor %}
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        {% endblock %}
